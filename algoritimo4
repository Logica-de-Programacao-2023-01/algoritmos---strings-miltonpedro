package main

import "fmt"

func main() {
	var primeira, segunda string
	fmt.Print("Digite uma string:")
	fmt.Scan(&primeira)

	fmt.Print("Digite outra string:")
	fmt.Scan(&segunda)
	if primeira == segunda {
		fmt.Print("As duas strings informadas são iguais!")
	} else {
		fmt.Print("As duas strings informadas são diferentes!")
	}
}
